@model string
@{
    ViewData["Title"] = "RegistrationSuccessful";
}
<div class="container body-content">
    <div class="row text-center" style="margin-top:35%">
        <div class="col-lg-12">
            <h2 class="text-success">Please check your inbox for our email and link to validate your account</h2>
            <h5>If you have not recieved account activation email please <a asp-controller="Account" asp-action="SendAccountActivationMail" asp-route-emailId="@Model">click here</a></h5>
        </div>
    </div>
</div>


